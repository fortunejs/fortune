{
  "name": "fortune",
  "description": "Data interface library.",
  "version": "1.0.0-alpha.1",
  "license": "MIT",
  "author": "Dali Zheng <d@liwa.li> (http://daliwa.li)",
  "contributors": [],
  "homepage": "http://fortunejs.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:fortunejs/fortune.git"
  },
  "bugs": "https://github.com/fortunejs/fortune/issues",
  "scripts": {
    "clean": "rimraf dist coverage",
    "coverage": "npm run build && istanbul cover dist/test",
    "deploy": "npm run deploy:website && npm run publish",
    "deploy:website": "npm run build:website && babel-node website/deploy",
    "build": "npm run build:lib && npm run build:test",
    "build:lib": "mkdirp dist/lib && babel lib --out-dir dist/lib",
    "build:test": "mkdirp dist/test && babel test --out-dir dist/test",
    "build:website": "babel-node website/build",
    "lint": "eslint lib test",
    "prepublish": "npm run build:lib",
    "test": "npm run lint && npm run build && REPORTER=true node dist/test | tap-dot"
  },
  "dependencies": {
    "babel-runtime": "5.4.7",
    "inflection": "1.7.1",
    "mongodb": "2.0.33",
    "nedb": "1.1.2",
    "negotiator": "0.5.3",
    "uri-templates": "0.1.7"
  },
  "devDependencies": {
    "babel": "5.4.7",
    "chalk": "1.0.0",
    "eslint": "0.21.2",
    "istanbul": "0.3.14",
    "mkdirp": "0.5.1",
    "node-fetch": "1.2.1",
    "rimraf": "2.3.4",
    "tap-dot": "1.0.0",
    "tape": "4.0.0"
  },
  "files": [
    "dist/lib",
    "lib/",
    "README.md",
    "LICENSE"
  ],
  "main": "dist/lib/index.js",
  "engines": {
    "node": ">=0.12"
  },
  "keywords": [
    "adapter",
    "api",
    "data",
    "hypermedia",
    "serializer"
  ]
}
